{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "declaration": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "outDir": "publish",
    "jsx": "preserve",
    "jsxFactory": "h",
    "importsNotUsedAsValues": "error",
  },
  "include": [
    "src"
  ]
}
